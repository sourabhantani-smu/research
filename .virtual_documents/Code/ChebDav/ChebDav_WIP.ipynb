            H=np.column_stack([H,V[:,kc:ksub+1].transpose().dot(w_knext)])
        for i in range(H.shape[0]-1):
            H[-1,i]=H[i,-1]
        #display(H)
        if verbose > 0:
            print('V', V.shape)
            print('W', W.shape)
            print('H', H.shape)
        D,Y = la.eigh(H[kc:ksub+1, kc:ksub+1])
        #display(D)
        if verbose > 0:
            print('Y',Y.shape)
