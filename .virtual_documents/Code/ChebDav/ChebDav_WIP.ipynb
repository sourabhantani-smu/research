import numpy as np

def generate_linearly_independent_vectors(n):
    vectors = []
    for i in range(n):
        vector = np.random.randn(n)
        for j in range(i):
            vector -= np.dot(vector, vectors[j]) / np.dot(vectors[j], vectors[j]) * vectors[j]
        vectors.append(vector)
    return vectors

